		"/cla/check": {
			"get": {
				"tags": ["CLA"],
				"summary": "Check CLA",
				"description": "Checks if one or multiple contributors have signed the CLA.",
				"operationId": "check_cla_cla_check_get",
				"parameters": [
					{
						"name": "emails",
						"in": "query",
						"required": false,
						"schema": {
							"anyOf": [
								{ "type": "array", "items": { "type": "string" }, "maxItems": 100 },
								{ "type": "null" }
							],
							"title": "Emails",
							"description": "A list of emails to check for CLA signatories",
							"examples": ["dev1@ubuntu.com,dev2@example.com"],
							"default": []
						},
						"description": "A list of emails to check for CLA signatories"
					},
					{
						"name": "github_usernames",
						"in": "query",
						"required": false,
						"schema": {
							"anyOf": [
								{ "type": "array", "items": { "type": "string" }, "maxItems": 100 },
								{ "type": "null" }
							],
							"title": "GitHub Usernames",
							"description": "A list of GitHub usernames to check for CLA signatories",
							"examples": ["dev1,dev2"],
							"default": []
						},
						"description": "A list of GitHub usernames to check for CLA signatories"
					},
					{
						"name": "launchpad_usernames",
						"in": "query",
						"required": false,
						"schema": {
							"anyOf": [
								{ "type": "array", "items": { "type": "string" }, "maxItems": 100 },
								{ "type": "null" }
							],
							"title": "Launchpad Usernames",
							"description": "A list of Launchpad usernames to check for CLA signatories",
							"examples": ["dev1,dev2"],
							"default": []
						},
						"description": "A list of Launchpad usernames to check for CLA signatories"
					}
				],
				"responses": {
					"200": {
						"description": "Successful Response",
						"content": {
							"application/json": { "schema": { "$ref": "#/components/schemas/CLACheckResponse" } }
						}
					},
					"422": {
						"description": "Validation Error",
						"content": {
							"application/json": {
								"schema": { "$ref": "#/components/schemas/HTTPValidationError" }
							}
						}
					}
				}
			}
		},
		"/cla/individual/sign": {
			"post": {
				"tags": ["CLA"],
				"summary": "Sign Individual CLA",
				"description": "Signs the CLA as an individual contributor.\n\n__Note:__\nThe user must have a valid `github_oauth2_session` and `launchpad_oauth_session` cookie sessions in order to verify their email addresses.",
				"operationId": "sign_cla_individual_cla_individual_sign_post",
				"requestBody": {
					"content": {
						"application/json": {
							"schema": { "$ref": "#/components/schemas/IndividualCreateForm" }
						}
					},
					"required": true
				},
				"responses": {
					"201": {
						"description": "Successful Response",
						"content": {
							"application/json": {
								"schema": { "$ref": "#/components/schemas/IndividualCreationSuccess" }
							}
						}
					},
					"422": {
						"description": "Validation Error",
